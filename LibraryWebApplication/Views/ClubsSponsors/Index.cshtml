@model IEnumerable<LibraryWebApplication.Models.ClubsSponsor>

@{
    ViewData["Title"] = "Index";
}

<h1>Спонсування</h1>

<p>
    <a asp-action="Create">Додати спонсора для клубу</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Club)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EdrpouNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Club.NameClub)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EdrpouNavigation.NameSponsor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ClubSponsorId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ClubSponsorId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ClubSponsorId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
